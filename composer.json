{
  "name": "pollen-solutions/support",
  "description": "Pollen Solutions - Support Component - Collection of PHP development support tools.",
  "homepage": "https://www.presstify.com/pollen-solutions/support/",
  "keywords": [
    "pollen-solutions",
    "component",
    "support",
    "PHP developpement"
  ],
  "time": "2022-03-30",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "composer/composer": "^2.0",
    "illuminate/support": "^8.22",
    "pollen-solutions/validation": "^1.0",
    "nesbot/carbon": "^2.31",
    "ramsey/uuid": "^4.0",
    "symfony/filesystem": "^5.2",
    "vlucas/phpdotenv": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Solutions - Container Component - PSR-11 Dependency injection container."
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Support\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Support\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev"
}
