{
  "name": "pollen-solutions/support",
  "description": "Pollen Support Component - Collection of PHP developpement support tools.",
  "homepage": "https://www.presstify.com/pollen-solutions/support/",
  "keywords": [
    "pollen-solutions",
    "component",
    "support",
    "PHP developpement"
  ],
  "time": "2021-07-05",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "composer/composer": "^2.0",
    "illuminate/support": "^8.22",
    "pollen-solutions/validation": "^1.0",
    "ramsey/uuid": "^4.0",
    "symfony/filesystem": "^5.2",
    "vlucas/phpdotenv": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Support\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Support\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
